<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Servlet Project</display-name>

   <servlet>
    <servlet-name>RegionGetServlet</servlet-name>
    <servlet-class>chaplinskiy.servlet.web.regionServlet.GetRegionServlet</servlet-class>
  </servlet>

    <servlet>
        <servlet-name>RegionGetAllServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.regionServlet.GetAllRegionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteRegionServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.regionServlet.DeleteRegionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreateRegionServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.regionServlet.CreateRegionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UpdateRegionServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.regionServlet.UpdateRegionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetAllPostsServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.postServlet.GetAllPostsServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetPostServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.postServlet.GetPostServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeletePostServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.postServlet.DeletePostServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreatePostServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.postServlet.CreatePostServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UpdatePostServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.postServlet.UpdatePostServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetUserServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.userServlet.GetUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetAllUserServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.userServlet.GetAllUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteUserServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.userServlet.DeleteUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreateUserServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.userServlet.CreateUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UpdateUserServlet</servlet-name>
        <servlet-class>chaplinskiy.servlet.web.userServlet.UpdateUserServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegionGetServlet</servlet-name>
        <url-pattern>/getRegion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegionGetAllServlet</servlet-name>
        <url-pattern>/getAllRegion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteRegionServlet</servlet-name>
        <url-pattern>/deleteRegion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreateRegionServlet</servlet-name>
        <url-pattern>/createRegion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateRegionServlet</servlet-name>
        <url-pattern>/updateRegion</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetAllPostsServlet</servlet-name>
        <url-pattern>/getAllPosts</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetPostServlet</servlet-name>
        <url-pattern>/getPost</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeletePostServlet</servlet-name>
        <url-pattern>/deletePost</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreatePostServlet</servlet-name>
        <url-pattern>/createPost</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdatePostServlet</servlet-name>
        <url-pattern>/updatePost</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetUserServlet</servlet-name>
        <url-pattern>/getUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetAllUserServlet</servlet-name>
        <url-pattern>/getAllUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteUserServlet</servlet-name>
        <url-pattern>/deleteUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreateUserServlet</servlet-name>
        <url-pattern>/createUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateUserServlet</servlet-name>
        <url-pattern>/updateUserServlet</url-pattern>
    </servlet-mapping>

</web-app>
